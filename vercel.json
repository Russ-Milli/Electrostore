{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "@vercel/php@0.7.0"
    }
  },
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/css/(.*)",
      "dest": "/public/assets/css/$1"
    },
    {
      "src": "/assets/js/(.*)",
      "dest": "/public/assets/js/$1"
    },
    {
      "src": "/assets/img/(.*)",
      "dest": "/public/assets/img/$1"
    },
    {
      "src": "/assets/lib/owlcarousel/(.*)",
      "dest": "/public/assets/lib/owlcarousel/$1"
    },
    {
      "src": "/assets/lib/(.*)",
      "dest": "/public/assets/lib/$1"
    },
    {
      "src": "/assets/scss/(.*)",
      "dest": "/public/assets/scss/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/public/img/$1"
    },
    {
      "src": "/vendor/(.*)",
      "dest": "/public/vendor/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/(.*\\.(html|htm))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Authorization"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "PHP_VERSION": "8.2"
  },
  "regions": ["iad1"]
}