{
  "version": 2,
  "functions": {
    "*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "^/public/(.*)$", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "^/(.*\\.php)$", "dest": "/$1" },
    { "src": "/.*", "dest": "/index.php" }
  ],
  "buildCommand": "composer install --no-dev --optimize-autoloader"

  
}
